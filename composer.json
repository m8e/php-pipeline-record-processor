{
  "name": "rodrigopedra/record-processor",
  "description": "Record Processor",
  "keywords": [
    "record processor",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Pedra Brum",
      "email": "rodrigo.pedra@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "psr/log": "^1.0",
    "illuminate/contracts": "^7.0|^8.0",
    "illuminate/support": "^7.0|^8.0",
    "illuminate/pipeline": "^7.0|^8.0",
    "league/csv": "^9.6",
    "phpoffice/phpspreadsheet": "^1.1"
  },
  "require-dev": {
    "symfony/console": "^5.1",
    "symfony/process": "^5.1",
    "symfony/var-dumper": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "RodrigoPedra\\RecordProcessor\\": "src/",
      "RodrigoPedra\\RecordProcessor\\Examples\\": "examples/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "suggest": {
    "ext-fileinfo": "to use file based readers/writers",
    "ext-json": "to use JSON writer",
    "ext-pdo": "to use database reader/writer"
  }
}
